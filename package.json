{
  "name": "@picocss/picocss.com",
  "version": "0.0.1",
  "author": "Lucas Larroche",
  "homepage": "https://picocss.com",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\" --silent && remix build ",
    "build:prettier": "prettier --write --loglevel silent 'app/**/*' '!app/styles/css/*'",
    "build:sort-scss": "postcss --config app/styles/scss ./app/styles/**/*.scss --replace",
    "build:css": "sass --load-path=node_modules/@picocss/pico/scss/ --load-path=node_modules/@picocss/pico/scss/settings --load-path=node_modules/@picocss/colors/scss app/styles/scss/:app/styles/css/",
    "build:autoprefix": "postcss --config app/styles/css --replace app/styles/css/*.css !app/styles/css/*.min.css",
    "build:minify": "cleancss -O1 --with-rebase --batch --batch-suffix .min app/styles/css/*.css !app/styles/css/*.min.css",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:remix": "remix watch",
    "dev:prettier": "onchange 'app/**/*' -e 'app/styles/css/*' -- prettier --write --loglevel silent {{changed}}",
    "dev:sort-scss": "onchange 'app/styles/**/*.scss' -- postcss --config app/styles/scss {{changed}} --replace",
    "dev:css": "sass --load-path=node_modules/@picocss/pico/scss/ --load-path=node_modules/@picocss/pico/scss/settings --load-path=node_modules/@picocss/colors/scss app/styles/scss/:app/styles/css/ --watch",
    "dev:autoprefix": "onchange 'app/styles/css/*.css' -e 'app/styles/css/*.min.css' -- postcss --config app/styles/css {{changed}} --replace",
    "dev:minify": "cleancss -O1 --with-rebase --batch --batch-suffix .min --watch app/styles/css/*.css !app/styles/css/*.min.css",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "wrangler": "wrangler pages dev ./public",
    "prebuild:prettier": "echo '[@picocss/picocss.com] ✨ Lint'",
    "prebuild:css": "echo '[@picocss/picocss.com] ✨ Compile'",
    "prebuild:autoprefix": "echo '[@picocss/picocss.com] ✨ Autoprefix'",
    "prebuild:minify": "echo '[@picocss/picocss.com] ✨ Minify'"
  },
  "dependencies": {
    "@picocss/colors": "github:picocss/colors",
    "@picocss/pico": "https://github.com/picocss/pico#v2",
    "@remix-run/cloudflare": "^1.9.0",
    "@remix-run/cloudflare-pages": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "1.3.0",
    "react-syntax-highlighter": "15.5.0",
    "use-local-storage-state": "18.1.2",
    "use-prefers-color-scheme": "1.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "autoprefixer": "^10.4.13",
    "caniuse-lite": "1.0.30001441",
    "clean-css-cli": "^5.6.1",
    "css-declaration-sorter": "^6.3.1",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "not ie > 0"
  ]
}
